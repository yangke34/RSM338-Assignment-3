    "from sklearn.linear_model import Ridge, Lasso\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "# 1. Use the 'train' DataFrame you just showed me to ensure X and y match\n",
    "# Drop any rows with missing values to be safe\n",
    "train_clean = train.dropna()\n",
    "\n",
    "# Split into y and X from the SAME source\n",
    "y_tr = train_clean['ExRet']\n",
    "X_tr = train_clean[multi_predictors]\n",
    "\n",
    "# 2. Standardize\n",
    "scaler = StandardScaler()\n",
    "X_tr_scaled = scaler.fit_transform(X_tr)\n",
    "\n",
    "# 3. Fit Ridge\n",
    "ridge_model = Ridge(alpha=1.0) \n",
    "ridge_model.fit(X_tr_scaled, y_tr) # These will now both be length 1175\n",
    "\n",
    "# 4. Fit Lasso\n",
    "lasso_model = Lasso(alpha=0.001)\n",
    "lasso_model.fit(X_tr_scaled, y_tr)"
   ]
